<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}Move On{% endblock %}</title>
        <link rel="stylesheet" href="/styles/vendors.css" />
        <link rel="stylesheet" href="/styles/main.css" />
    </head>
    <body class="{% block bodyclass %}{% endblock %}">
        <div style="display:none">{% include 'components/symbol/svg/sprite.symbol.svg' %}</div>
        <div class="wrapper">
          {% include 'components/header' %}
          <div id="content" class="container">{% block content %}{% endblock %}</div>
          {% include 'components/footer' %}
        </div>
        <script src="/scripts/vendors.js"></script>
        <script src="/scripts/app.js"></script>
    </body>
</html>
